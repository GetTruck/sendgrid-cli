{
  "name": "sendgrid-cli",
  "description": "A full-fledge CLI for SendGrid API",
  "version": "0.0.1",
  "author": "Shailen Naidoo",
  "bin": {
    "sendgrid-cli": "./bin/run"
  },
  "bugs": "https://github.com/GetTruck/sendgrid-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/cryptr": "^4.0.0",
    "@types/fs-extra": "^7.0.0",
    "cryptr": "^4.0.2",
    "fs-extra": "^8.0.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "globby": "^8",
    "rimraf": "^2.6.3",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/GetTruck/sendgrid-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sendgrid-cli",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "apikey": {
        "description": "Manage your SendGrid API KEY"
      },
      "contacts": {
        "description": "Manage your contacts"
      }
    }
  },
  "repository": "https://github.com/GetTruck/sendgrid-cli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
